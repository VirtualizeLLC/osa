{
  "version": "1.0",
  "description": "Example OSA configuration - customize and save to use with './osa-cli.zsh --config <file>'",
  "profile": "web",
  "components": {
    "symlinks": true,
    "oh_my_zsh": true,
    "zsh_plugins": true,
    "homebrew": true,
    "mise": true,
    "osa_snippets": true,
    "android": false,
    "iterm2": false,
    "vscode": false
  },
  "runtimes": {
    "node": {
      "enabled": true,
      "version": "22"
    },
    "python": {
      "enabled": false,
      "version": "3.13"
    },
    "ruby": {
      "enabled": false,
      "version": "3.4.0"
    },
    "java": {
      "enabled": false,
      "version": "openjdk-21"
    },
    "rust": {
      "enabled": false,
      "version": "stable"
    },
    "go": {
      "enabled": false,
      "version": "1.23"
    },
    "deno": {
      "enabled": false,
      "version": "latest"
    },
    "elixir": {
      "enabled": false,
      "version": "latest"
    },
    "erlang": {
      "enabled": false,
      "version": "latest"
    }
  }
}
