{
  "version": "1.0",
  "description": "Backend/polyglot development setup (Node, Python, Ruby, Java, Go, Rust)",
  "profile": "backend",
  "components": {
    "symlinks": true,
    "oh_my_zsh": true,
    "zsh_plugins": true,
    "homebrew": true,
    "mise": true,
    "osa_snippets": true,
    "git": true,
    "android": false,
    "cocoapods": false,
    "iterm2": true,
    "vscode": true
  },
  "runtimes": {
    "node": {
      "enabled": true,
      "version": "22"
    },
    "python": {
      "enabled": true,
      "version": "3.13"
    },
    "ruby": {
      "enabled": true,
      "version": "3.4.0"
    },
    "java": {
      "enabled": true,
      "version": "openjdk-21"
    },
    "rust": {
      "enabled": true,
      "version": "stable"
    },
    "go": {
      "enabled": true,
      "version": "1.23"
    },
    "deno": {
      "enabled": false,
      "version": "latest"
    },
    "elixir": {
      "enabled": true,
      "version": "latest"
    },
    "erlang": {
      "enabled": true,
      "version": "latest"
    }
  }
}
