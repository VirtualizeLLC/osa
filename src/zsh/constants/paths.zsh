#!/usr/bin/env zsh
# Repository-wide constants for Open Source Automation (OSA)
export OSA_CONFIG=$HOME/.osa
export OSA_EXTERNAL_LIBS=$OSA_CONFIG/external-libs
export OSA_ZSH_CONSTRUCTORS=$OSA_CONFIG/src/zsh/constructors
export OSA_ZSH_PLUGINS=$OSA_CONFIG/src/zsh/plugin-init
export OSA_ZSH_ALIASES=$OSA_CONFIG/src/zsh/aliases
export OSA_ZSH_CONSTANTS=$OSA_CONFIG/src/zsh/constants
export OSA_CONFIG_APPS=$OSA_CONFIG/src/apps
export OSA_CONFIG_VERBOSE=false
export OSA_OH_MY_ZSH_DIR=$OSA_EXTERNAL_LIBS/.oh-my-zsh
export OSA_ZSHRC=$OSA_CONFIG/src/zsh/.zshrc